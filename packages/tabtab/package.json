{
    "name": "@react-tabtab-next/tabtab",
    "version": "3.0.6",
    "description": "[TypeScript] A mobile support, draggable, editable and api based Tab for ReactJS",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rimraf dist && rollup -c ../../rollup.config.js",
        "validate": "npm ls",
        "demo": "npx webpack serve --config ./demo/webpack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/onmotion/react-tabtab-next.git",
        "directory": "packages/tabtab"
    },
    "keywords": [
        "react",
        "tabs",
        "drag",
        "react-tabtab",
        "react-component",
        "tab",
        "tabtab",
        "tabtab-next",
        "react-tabtab-next",
        "typescript",
        "react draggable tabs"
    ],
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "author": {
        "name": "Alexandr Kozhevnikov",
        "email": "onmotion1@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/onmotion/react-tabtab-next/issues"
    },
    "homepage": "https://github.com/onmotion/react-tabtab-next#readme",
    "dependencies": {
        "classnames": "^2.2.5",
        "invariant": "^2.2.2",
        "react-icons": "^4.3.1",
        "react-modal": "^3.14.4",
        "react-sortable-hoc": "^2.0.0",
        "react-transition-group": "^4.4.2",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/estree": "^0.0.51",
        "@types/invariant": "^2.2.35",
        "@types/react-modal": "^3.13.1",
        "@types/styled-components": "^5.1.24",
        "babel-loader": "^8.2.4",
        "html-webpack-plugin": "^5.5.0",
        "rimraf": "^2.6.2",
        "ts-loader": "^9.2.8",
        "tslib": "^2.3.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.0"
    },
    "peerDependencies": {
        "@types/react": ">=16.3.0",
        "react": ">=16.3.0",
        "react-dom": ">=16.3.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "jest": {
        "setupFiles": [
            "./test/shim",
            "./test/enzyme-setup"
        ],
        "roots": [
            "<rootDir>/test/"
        ],
        "unmockedModulePathPatterns": [
            "node_modules/react/",
            "node_modules/enzyme/"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "pre-commit": [
        "lint",
        "test"
    ]
}
