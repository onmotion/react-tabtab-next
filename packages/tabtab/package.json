{
  "name": "@react-tabtab-next/tabtab",
  "version": "3.0.0",
  "description": "A mobile support, draggable, editable and api based Tab for ReactJS",
  "main": "dist/index.js",
  "typings": "src/",
  "scripts": {
    "start": "node devServer.js",
    "build": "rimraf dist && rollup -c ../../rollup.config.js",
    "lint": "eslint src example test",
    "test": "jest",
    "test:watch": "NODE_ENV=test npm test -- --watch",
    "flow": "flow check",
    "flow:watch": "flow-watch",
    "gh-pages": "rimraf _gh-pages && npm run gh-pages:build && npm run gh-pages:publish",
    "gh-pages:build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "gh-pages:publish": "git-directory-deploy --directory _gh-pages --branch gh-pages",
    "prepublish": "npm run build",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmotion/react-tabtab-next.git",
    "directory": "packages/tabtab"
  },
  "keywords": [
    "react",
    "tabs",
    "react-tab",
    "react-tabtab",
    "react-component",
    "tab",
    "tabtab"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "onmotion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onmotion/react-tabtab-next/issues"
  },
  "homepage": "https://github.com/onmotion/react-tabtab-next#readme",
  "dependencies": {
    "react-sortable-hoc": "^2.0.0",
    "@types/estree": "^0.0.51",
    "@types/react": "^17.0.42",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^9.2.0",
    "invariant": "^2.2.2",
    "prop-types": "^15.6.0",
    "react-icons": "^4.3.1",
    "react-poppop": "^1.5.0",
    "react-select": "^5.2.2",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^4.4.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.5",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.24",
    "babel-cli": "^6.26.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.7.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^8.12.0",
    "eslint-config-google": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-styled-components": "^6.3.0",
    "noop3": "^999.999.999",
    "precommit-hook-eslint": "^3.0.0",
    "prettier": "^1.7.4",
    "react": ">=16.3.0",
    "react-content-loader": "^3.1.1",
    "rimraf": "^2.6.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "peerDependencies": {
    "react": ">= 16.3.0"
  },
  "jest": {
    "setupFiles": [
      "./test/shim",
      "./test/enzyme-setup"
    ],
    "roots": [
      "<rootDir>/test/"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
