{
  "name": "@react-tabtab-next/tabtab",
  "version": "3.0.0",
  "description": "A mobile support, draggable, editable and api based Tab for ReactJS",
  "main": "dist/bundle.min.js",
  "typings": "src/",
  "scripts": {
    "build": "rimraf dist && rollup -c ../../rollup.config.js",
    "lint": "eslint .",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmotion/react-tabtab-next.git",
    "directory": "packages/tabtab"
  },
  "keywords": [
    "react",
    "tabs",
    "react-tab",
    "react-tabtab",
    "react-component",
    "tab",
    "tabtab",
    "tabtab-next",
    "react-tabtab-next"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "onmotion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onmotion/react-tabtab-next/issues"
  },
  "homepage": "https://github.com/onmotion/react-tabtab-next#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "invariant": "^2.2.2",
    "react-icons": "^4.3.1",
    "react-poppop": "^1.5.0",
    "react-sortable-hoc": "^2.0.0",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/estree": "^0.0.51",
    "@types/react": ">= 16.3.0",
    "@types/styled-components": "^5.1.24",
    "eslint": "^8.12.0",
    "eslint-config-google": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-react": "*",
    "husky": "^0.14.3",
    "jest": "^27.5.1",
    "noop3": "^999.999.999",
    "precommit-hook-eslint": "^3.0.0",
    "rimraf": "^2.6.2",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^16.3.0 || ^17.0.0",
    "react-dom": "^16.3.0 || ^17.0.0"
  },
  "jest": {
    "setupFiles": [
      "./test/shim",
      "./test/enzyme-setup"
    ],
    "roots": [
      "<rootDir>/test/"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
